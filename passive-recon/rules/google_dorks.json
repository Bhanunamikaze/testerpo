{
  "version": "1.0.0",
  "description": "Comprehensive Google Dorks for Passive Reconnaissance",
  "categories": {
    "sensitive_files": {
      "description": "Configuration files, credentials, and sensitive data",
      "severity": "high",
      "dorks": [
        {
          "query": "site:{domain} ext:env OR filetype:env",
          "description": "Environment configuration files",
          "keywords": ["API_KEY", "SECRET", "PASSWORD", "TOKEN"]
        },
        {
          "query": "site:{domain} ext:sql OR filetype:sql",
          "description": "SQL database dumps",
          "keywords": ["INSERT INTO", "CREATE TABLE", "password"]
        },
        {
          "query": "site:{domain} ext:log OR filetype:log",
          "description": "Log files with potential sensitive data",
          "keywords": ["error", "exception", "stack trace", "password"]
        },
        {
          "query": "site:{domain} ext:bak OR ext:old OR ext:backup",
          "description": "Backup files",
          "keywords": ["backup", "db", "database"]
        },
        {
          "query": "site:{domain} ext:config OR ext:conf OR ext:cfg",
          "description": "Configuration files",
          "keywords": ["password", "secret", "key"]
        },
        {
          "query": "site:{domain} filetype:json \"firebase\"",
          "description": "Firebase configuration",
          "keywords": ["apiKey", "authDomain", "databaseURL"]
        },
        {
          "query": "site:{domain} ext:xml \"backup\"",
          "description": "XML backup files",
          "keywords": ["database", "config"]
        },
        {
          "query": "site:{domain} ext:yaml OR ext:yml",
          "description": "YAML configuration files",
          "keywords": ["secret", "password", "token", "key"]
        },
        {
          "query": "site:{domain} filetype:properties",
          "description": "Java properties files",
          "keywords": ["password", "jdbc", "database"]
        },
        {
          "query": "site:{domain} \".npmrc\" OR \".pypirc\"",
          "description": "Package manager configuration with tokens",
          "keywords": ["token", "auth"]
        }
      ]
    },
    "credentials": {
      "description": "API keys, tokens, passwords, and authentication data",
      "severity": "critical",
      "dorks": [
        {
          "query": "site:{domain} intext:\"api_key\" OR intext:\"apikey\" OR intext:\"api-key\"",
          "description": "API keys in content",
          "keywords": ["api_key", "apikey"]
        },
        {
          "query": "site:{domain} intext:\"access_key\" OR intext:\"secret_key\"",
          "description": "Access and secret keys",
          "keywords": ["AWS", "S3"]
        },
        {
          "query": "site:{domain} intext:\"BEGIN RSA PRIVATE KEY\" OR intext:\"BEGIN OPENSSH PRIVATE KEY\"",
          "description": "SSH private keys",
          "keywords": ["PRIVATE KEY"]
        },
        {
          "query": "site:{domain} intext:\"Authorization: Bearer\"",
          "description": "Bearer tokens",
          "keywords": ["Bearer", "token"]
        },
        {
          "query": "site:{domain} \"AWS_ACCESS_KEY_ID\" OR \"AWS_SECRET_ACCESS_KEY\"",
          "description": "AWS credentials",
          "keywords": ["AKIA"]
        },
        {
          "query": "site:{domain} \"GOOGLE_APPLICATION_CREDENTIALS\" OR \"FIREBASE_API_KEY\"",
          "description": "Google/Firebase credentials",
          "keywords": ["AIza"]
        },
        {
          "query": "site:{domain} \"AZURE_STORAGE_CONNECTION_STRING\" OR \"AZURE_CLIENT_SECRET\"",
          "description": "Azure credentials",
          "keywords": ["DefaultEndpointsProtocol"]
        },
        {
          "query": "site:{domain} \"token\" \"expires_in\"",
          "description": "OAuth tokens",
          "keywords": ["access_token", "refresh_token"]
        },
        {
          "query": "site:{domain} \"client_secret\" OR \"client_id\"",
          "description": "OAuth client credentials",
          "keywords": ["client_secret"]
        },
        {
          "query": "site:{domain} \"ssh-rsa\"",
          "description": "SSH public keys (may lead to private keys)",
          "keywords": ["ssh-rsa"]
        },
        {
          "query": "site:{domain} \"X-API-KEY\" OR \"X-Auth-Token\"",
          "description": "API authentication headers",
          "keywords": ["X-API-KEY"]
        },
        {
          "query": "site:{domain} (\"password\" OR \"passwd\" OR \"pwd\") AND (\"=\" OR \":\")",
          "description": "Password assignments in code",
          "keywords": ["password"]
        },
        {
          "query": "site:{domain} \"connection string\" \"password\"",
          "description": "Database connection strings",
          "keywords": ["Server", "Database", "User", "Password"]
        }
      ]
    },
    "admin_panels": {
      "description": "Administrative interfaces and dashboards",
